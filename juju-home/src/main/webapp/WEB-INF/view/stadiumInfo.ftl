		var values = GewaraUtil.getValues("orderForm");
		var err = "",val=$("mobile").value.trim();
		if(val == ""){err += "请输入您的电话号码！"}
		if((!GewaraUtil.isNumber(val) || val.length != 11) && val != "") err += "请输入正确的手机号码！";
		if(err.length>0){gewaUtil.alert(err); return;}
		gewa.util.mask({'element': el,'title':'正在提交，请稍等...'});
		GewaraUtil.sendRequest(url, values, function(result){
			gewa.util.clearMask();
			if(result.success){
				document.location.href="/gewapay/order.xhtml?orderId=" + result.retval;
			}else{
				GewaraUtil.refreshCaptcha('captcha','','https://www.gewara.com/');
				gewaUtil.alert(result.msg);
				
			}
		});
	});
}
