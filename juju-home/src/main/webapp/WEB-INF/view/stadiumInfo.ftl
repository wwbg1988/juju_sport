/*人次预订*/
function duration(obj){
	var iSum=obj.get('date-sum');
	$('quantitySum').innerHTML='';
	$$('.duration span').each(function(item){item.removeClass('select');})
	obj.addClass('select');
	var html = '';
	for(var i=0;i<iSum;i++){
		var s=(i+1);
		html += "<a date-id='"+s+"' href='javascript:void(0);' onclick='changeQuantity(this)'>"+s+"</a>";
	}
	$('quantitySum').innerHTML='<font>选择票数：</font>'+html;
	$('selecttime').value=obj.get('date-date')+'&nbsp;'+obj.get('date-time');
	$('otiid').value=obj.get('lang');
	$('iPrice').value=obj.get('date-price');
	$('quantity').value=''; 
	changeQuantityBox('on');
}
function changeQuantity(obj){
	var quantity = $(obj).get('date-id');
	var tmpHourPrice = $('iPrice').value;
	var pos=obj.getPosition(),hasSiteBox=$('hasSiteBox');
	$('quantity').value=quantity;
	$('showPrice').innerHTML=quantity*tmpHourPrice;
	$$('.quantitySum a').each(function(item){item.removeClass('select');})
	obj.addClass('select');
	changeQuantityBox();
	var issite = new Element('span',{'styles':{'top':pos.y,'left':pos.x,'position':'absolute'},'html':$('selecttime').value+'&nbsp;<em id="showpn">'+$('quantity').value+'</em>人'}).addClass('isSite fs12').inject(document.body);
	obj.store('key',issite);
	issite.set('move', {duration: '400',onComplete:function(){
		if(obj.retrieve('key') != null){
			var isel = obj.retrieve('key');
			hasSiteBox.innerHTML='';
			isel.inject(hasSiteBox);
			isel.set('style','');
		}
	}.bind(this)}).move({
		relativeTo:hasSiteBox,
		position:"center",
   	offset:{x:45,y:0}
   })
   $('pubType').setStyle('display','block');
}
function changeQuantityBox(on){
	if(on=='on')$('hasSiteBox').innerHTML='';
	$('cancelSite').setStyle('display',on=='on'?'block':'none');
	$('hasSiteBox').setStyle('display',on=='on'?'none':'block');
}
